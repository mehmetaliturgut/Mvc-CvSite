@model MvcCV.Models.Entity.tbl_Experience
@{
    ViewBag.Title = "ExpGet";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Update Experience Page</h4>

<br />

<form method="post">
    @Html.Label("Experience ID")
    @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
    <br />
    @Html.Label("Title")
    @Html.TextBoxFor(x => x.Ttile, new { @class = "form-control" })
    <br />
    @Html.Label("Subtitle")
    @Html.TextBoxFor(x => x.Subtitle, new { @class = "form-control" })
    <br />
    @Html.Label("Date")
    @Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
    <br />
    @Html.Label("Explain")
    @Html.TextAreaFor(x=>x.Explanation ,new {@class="form-control"})

    <button class="btn btn-dark" style="margin-top:15px;">Update</button>
</form>

