@using MvcCV.Models.Entity
@model List<tbl_Experience>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Experience List</h2>

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Subtitle</th>
        <th>Date</th>
        <th>Delete</th>
        <th>Edit</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Ttile</td>
            <td>@item.Subtitle</td>
            <td>@item.Date</td>
            <td><a href="/Experience/ExpDelete/@item.ID" class="btn btn-danger">Delete</a></td>
            <td><a href="/Experience/ExpGet/@item.ID" class="btn btn-warning">Edit</a></td>

        </tr>
    }
</table>
<a href="/Experience/ExpAdd/" class="btn btn-primary">Add New Experience</a>
